* {
	/*margin:0;*/
	padding:0;
}

/** General Style Info **/
body {
	/*background: #003d4c;*/
	background: #ECF0F6; 
	color: #000;
	/*font-family:'lucida grande',verdana,helvetica,arial,sans-serif;*/
	font-family:arial,sans-serif;
	font-size:90%;
	margin:0px;
}
a {
	color: #003d4c;
	text-decoration: underline;
	font-weight: bold;
}
a:hover {
	color: #367889;
	text-decoration:none;
    cursor:pointer;
}
a img {
	border:none;
}
h1, h2, h3, h4 {
	font-weight: normal;
	margin-bottom:0.5em;
}
h1 {
	background:#fff;
	color: #003d4c;
	font-size: 100%;
}
h2 {
	background:#fff;
	color: #e32;
	/*font-family:'Gill Sans','lucida grande', helvetica, arial, sans-serif;*/
	font-family:arial,sans-serif;
	font-size: 190%;
}
h3 {
	color: #2c6877;
	/*font-family:'Gill Sans','lucida grande', helvetica, arial, sans-serif;*/
	font-family:arial,sans-serif;
	font-size: 165%;
}
h4 {
     color: #993;
     font-weight: normal;
}
/*
h5 {
    color: #FF0000;
    font-weight: 90%;
}
*/
ul, li {
	margin: 0 12px;
    list-style-type: none;
}
p {
	margin: 0 0 1em 0;
}

#quotation {
    color: #FF0000;
    font-size: 80%;
}

