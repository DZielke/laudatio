}

#footer_no_logos:hover{
/*    -webkit-box-shadow: 1px 1px 1px #003d4c;
    box-shadow: 1px 1px 1px #003d4c;*/
}

/*footer_middle*/
#footer_middle {
    /*clear: both;*/
    padding: 3px 5px;
    text-align: right;
    margin-top: 0%;
    margin-left: 1%;
